<section class="vbox">
  <section class="scrollable padder">
    <ul class="breadcrumb no-border no-radius b-b b-light pull-in">
      <li><a href="/"><i class="fa fa-home"></i> Home</a></li>
      <li><a href="#">Definições</a></li>
      <li><a href="/equipe">Equipes</a></li>
      <li><a class="active">Adicionar</a></li>
    </ul>

    <form data-validate="parsley">
      <section class="panel panel-default">
        <header class="panel-heading">
          <span class="h4">Register</span>
        </header>
        <div class="panel-body">
          <div class="form-group pull-in clearfix">
            <div class="col-sm-6">
              <label>Nome</label>
              <input type="text" class="form-control" data-required="true" id="nome" placeholder="Nome da equipe">   
            </div>
            <div class="col-sm-6">
              <label>Sigla</label>
              <input type="text" class="form-control" data-required="sigla" id="sigla" placeholder="Sigla da equipe">      
            </div>   
          </div>
          <div class="form-group pull-in clearfix">
            <div class="col-sm-6">
              <label>Delegado</label>
              <input type="text" class="form-control" data-required="true" id="delegado" placeholder="Nome do Delegado">   
            </div>
            <div class="col-sm-6">
              <label>Contacto do Delegado</label>
              <input type="text" class="form-control" data-required="true" id="delCell" placeholder="Contacto do Delegado">      
            </div>   
          </div>
        </div>
        <footer class="panel-footer text-right bg-light lter">
          <button id="btnTerminar" class="btn btn-default btn-s-xs">Terminar</button>
          <button id="btnSalvar" class="btn btn-success btn-s-xs">Salvar</button>
        </footer>
      </section>
    </form>
  </section>
</section>

<script src="../js/jquery.min.js"></script>
<script src="../js/app/equipe/script.js"></script>